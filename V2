if game.PlaceId == 13359942345 then
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Wanz Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

-- Values
_G.autoCandy = true
_G.candyMulti = true

-- Function

function autoCandy()
while _G.autoCandy == true do
local args = {
    [1] = "Halloween"
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Teleport"):InvokeServer(unpack(args))
local rootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
rootPart.CFrame = workspace.Main.Candy:GetChildren()[5].CandyCorn.CFrame
    wait(.0000001)
end
end

function candyMulti()
_G.candyMulti == true
local args = {
    [1] = 2,
    [2] = "Event"
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Upgrade"):InvokeServer(unpack(args))'
end

local Tab = Window:MakeTab({
	Name = "OP MENU",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Auto Collect"
})

--[[
Name = <string> - The name of the section.
]]

OrionLib:MakeNotification({
	Name = "Yokoso",
	Content = "Watashi no soul society",
	Image = "rbxassetid://4483345998",
	Time = 5
})

-- Tabs
local FarmingTab = Window:MakeTab({
	Name = "Candy Farm",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

-- Toggles
FarmingTab:AddToggle({
	Name = "Auto Collect Candy",
	Default = false,
	Callback = function(Value)
		_G.autoCandy = Value
		autoCandy()
	end    
})

FarmingTab:AddToggle({
	Name = "Auto Upgrade Candy Multiplier",
	Default = false,
	Callback = function(Value)
		_G.candyMulti = Value
		candyMulti()
	end    
})











end
OrionLib:Init()
