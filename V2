if game.PlaceId == 13359942345 then
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Wanz Hub / [🎃HALLOWEEN!🎃] Stat Grinding Simulator 3 ⬆️", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

OrionLib:MakeNotification({
	Name = "Yokoso, Watashi No Soul Society",
	Content = "Yare Yare",
	Image = "rbxassetid://18539476040",
	Time = 30
})

-- Values
_G.Teleport = true 
_G.candyMulti = true
_G.statMulti = true
_G.Collect = true

-- Function

function Collect()
while _G.Collect == true do
firetouchinterest(game.Players.LocalPlayer.Character.UpperTorso, workspace.Main.Candy.Halloween.CandyCorn, 0)
wait()
end

function candyMulti()
   while _G.candyMulti == true do
     local args = {
    [1] = 2,
    [2] = "Event"
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Upgrade"):InvokeServer(unpack(args))
wait(0)
end
end

function statMulti()
   while _G.candyMulti == true do
     local args = {
    [1] = 1,
    [2] = "Event"
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Upgrade"):InvokeServer(unpack(args))
wait(0)
end
end
-- Tabs
FarmingTab = Window:MakeTab({
	Name = "Halloween Event 🎃",
	Icon = "rbxassetid://18539476040",
	PremiumOnly = false
})

FarmingTab:AddButton({
	Name = "Claim Achievement 🏆",
	Callback = function()
      		print("button pressed")
  	end    
})

FarmingTab:AddButton({
	Name = "Claim Daily 🗓️",
	Callback = function()
      		print("button pressed")
  	end    
})

FarmingTab:AddToggle({
	Name = "Auto Collect Candy 🍬",
	Default = false,
	Callback = function(Value)
		_G.Collect = Value
                Collect()
	end    
})

FarmingTab:AddToggle({
	Name = "Auto Upgrade Candy Multiplier 🎯",
	Default = false,
	Callback = function(Value)
		_G.candyMulti = Value
		candyMulti()
	end    
})

FarmingTab:AddToggle({
	Name = "Auto Upgrade Event Stat Multiplier 🎯",
	Default = false,
	Callback = function(Value)
		_G.statMulti = Value
		statMulti()
	end    
})

end
