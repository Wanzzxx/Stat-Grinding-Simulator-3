if game.PlaceId == 13359942345 then
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Wanz Hub / [üéÉHALLOWEEN!üéÉ] Stat Grinding Simulator 3 ‚¨ÜÔ∏è", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

--[[
Name = <string> - The name of the tab.
Icon = <string> - The icon of the tab.
PremiumOnly = <bool> - Makes the tab accessible to Sirus Premium users only.
]]

-- Values
_G.autoCandy = true 
_G.candyMulti = true
_G.statMulti = true

-- Function

function autoCandy()
   while _G.autoCandy == true do
     local args = {
    [1] = "Halloween"
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Teleport"):InvokeServer(unpack(args))
local rootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
rootPart.CFrame = workspace.Main.Candy:GetChildren()[5].CandyCorn.CFrame
wait(0)
end
end

function candyMulti()
   while _G.candyMulti == true do
     local args = {
    [1] = 2,
    [2] = "Event"
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Upgrade"):InvokeServer(unpack(args))
wait(0)
end
end

function statMulti()
   while _G.candyMulti == true do
     local args = {
    [1] = 1,
    [2] = "Event"
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Upgrade"):InvokeServer(unpack(args))
wait(0)
end
end
-- Tabs
local Tab = Window:MakeTab({
	Name = "Halloween Event üéÉ",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab = Window:MakeTab({
	Name = "Main Game [Not Finished]",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--[[
Name = <string> - The name of the tab.
Icon = <string> - The icon of the tab.
PremiumOnly = <bool> - Makes the tab accessible to Sirus Premium users only.
]]

local Tab:AddToggle({
	Name = "Auto Collect Candy",
	Default = false,
	Callback = function(Value)
		_G.autoCandy = Value
		autoCandy()
	end    
})

local Tab:AddToggle({
	Name = "Auto Upgrade Candy Multiplier",
	Default = false,
	Callback = function(Value)
		_G.candyMulti = Value
		candyMulti()
	end    
})

local Tab:AddToggle({
	Name = "Auto Upgrade Event Stat Multiplier",
	Default = false,
	Callback = function(Value)
		_G.statMulti = Value
		statMulti()
	end    
})

end
